#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/abrahamnyagar/Desktop/microservices/ticketing/payments/node_modules/.pnpm/node-nats-streaming@0.3.2/node_modules/node-nats-streaming/examples/node_modules:/home/abrahamnyagar/Desktop/microservices/ticketing/payments/node_modules/.pnpm/node-nats-streaming@0.3.2/node_modules/node-nats-streaming/node_modules:/home/abrahamnyagar/Desktop/microservices/ticketing/payments/node_modules/.pnpm/node-nats-streaming@0.3.2/node_modules:/home/abrahamnyagar/Desktop/microservices/ticketing/payments/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/abrahamnyagar/Desktop/microservices/ticketing/payments/node_modules/.pnpm/node-nats-streaming@0.3.2/node_modules/node-nats-streaming/examples/node_modules:/home/abrahamnyagar/Desktop/microservices/ticketing/payments/node_modules/.pnpm/node-nats-streaming@0.3.2/node_modules/node-nats-streaming/node_modules:/home/abrahamnyagar/Desktop/microservices/ticketing/payments/node_modules/.pnpm/node-nats-streaming@0.3.2/node_modules:/home/abrahamnyagar/Desktop/microservices/ticketing/payments/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../node-nats-streaming@0.3.2/node_modules/node-nats-streaming/examples/stan-pub.js" "$@"
else
  exec node  "$basedir/../../../../../../node-nats-streaming@0.3.2/node_modules/node-nats-streaming/examples/stan-pub.js" "$@"
fi
