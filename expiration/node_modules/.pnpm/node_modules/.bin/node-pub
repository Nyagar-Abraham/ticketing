#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/abrahamnyagar/Desktop/microservices/ticketing/expiration/node_modules/.pnpm/nats@1.4.12/node_modules/nats/examples/node_modules:/home/abrahamnyagar/Desktop/microservices/ticketing/expiration/node_modules/.pnpm/nats@1.4.12/node_modules/nats/node_modules:/home/abrahamnyagar/Desktop/microservices/ticketing/expiration/node_modules/.pnpm/nats@1.4.12/node_modules:/home/abrahamnyagar/Desktop/microservices/ticketing/expiration/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/abrahamnyagar/Desktop/microservices/ticketing/expiration/node_modules/.pnpm/nats@1.4.12/node_modules/nats/examples/node_modules:/home/abrahamnyagar/Desktop/microservices/ticketing/expiration/node_modules/.pnpm/nats@1.4.12/node_modules/nats/node_modules:/home/abrahamnyagar/Desktop/microservices/ticketing/expiration/node_modules/.pnpm/nats@1.4.12/node_modules:/home/abrahamnyagar/Desktop/microservices/ticketing/expiration/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../nats/examples/node-pub" "$@"
else
  exec node  "$basedir/../nats/examples/node-pub" "$@"
fi
